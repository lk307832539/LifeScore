<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.TaskItem" table="task_item" schema="lifescore">
        <id name="taskItemId">
            <column name="task_item_id" sql-type="int(11)"/>
        </id>
        <property name="taskId">
            <column name="task_id" sql-type="int(11)"/>
        </property>
        <property name="taskDetail">
            <column name="task_detail" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="taskByTaskId" class="com.entity.Task">
            <column name="task_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>