<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.TaskType" table="task_type" schema="lifescore">
        <id name="taskTypeId">
            <column name="task_type_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="taskTypeName">
            <column name="task_type_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="taskTypeScore">
            <column name="task_type_score" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="taskTypePriority">
            <column name="task_type_priority" sql-type="tinyint(4)" not-null="true"/>
        </property>
        <property name="taskTypeIsEnabled">
            <column name="task_type_is_enabled" sql-type="tinyint(4)" not-null="true"/>
        </property>
        <property name="taskTypeImg">
            <column name="task_type_img" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="userByUserId" class="com.entity.User">
            <column name="user_id"/>
        </many-to-one>
        <set name="tasksByTaskTypeId" inverse="true">
            <key>
                <column name="task_type_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.entity.Task"/>
        </set>
    </class>
</hibernate-mapping>