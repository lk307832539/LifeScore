<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.DayScoreLog" table="day_score_log" schema="lifescore">
        <id name="scoreLogId">
            <column name="score_log_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="totleScore">
            <column name="totle_score" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="addScore">
            <column name="add_score" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="cutScore">
            <column name="cut_score" sql-type="int(255)" not-null="true"/>
        </property>
        <property name="scoreLogDate">
            <column name="score_log_date" sql-type="datetime" not-null="true"/>
        </property>
        <many-to-one name="userByUserId" class="com.entity.User">
            <column name="user_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>